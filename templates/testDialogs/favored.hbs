<fieldset>
  <legend>{{localize 'torgeternity.skillDialogs.favoredanddisfavored'}}</legend>
  <input type="checkbox" data-dtype="Boolean" class="attack-dialog-checkbox" id="isFav" name="isFav"
    data-tooltip="{{localize 'torgeternity.skillDialogs.favoredTip'}}"
    value="true" {{checked test.isFav}}>
  <label for="isFav">{{localize 'torgeternity.skillDialogs.favored'}}</label>

  <input type="checkbox" data-dtype="Boolean" class="attack-dialog-checkbox" id="disfavored" name="disfavored"
    data-tooltip="{{localize 'torgeternity.skillDialogs.disfavoredTip'}}"
    value="false" {{checked test.disfavored}} />
  <label for="disfavored">{{localize 'torgeternity.skillDialogs.disfavored'}}</label>
</fieldset>