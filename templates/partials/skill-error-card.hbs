<div class="skill-card sheet-card" data-type="{{data.type}}"
    style="background-image: url('systems/torgeternity/images/bgparch.webp')">
    <table style="border:none;background-color:transparent;margin:2px 0px 0px 0px">
        <tr style="border:none">
            <td>
                <img class="actor-icon" src="{{actorPic}}" title="{{name}}" style="width:50px;height:50px" />
            </td>
            <td class="chat-title" style="font-size:18px;padding:0px 0px 0px 0px">
                {{localize "torgeternity.chatText.check.testUnsuccessfull"}}
            </td>
        </tr>
    </table>
    <div class="chat-text" style="padding:0px">
        <p style="text-align:center;font-size:14px;padding:3px 0px 0px 0px">{{message}}</p>
    </div>
</div>